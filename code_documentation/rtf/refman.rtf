    1. forward hash <- hash(start) + [hash(start + 1) \par
    for start = 0 to startpoints]\par
    2. backward hash <- hash(start) + [hash(start - 1) \par
    for start = startpoints to 0]\par
    3. Return forward hash + backward hash\par
